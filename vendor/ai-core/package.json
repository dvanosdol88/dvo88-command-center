{
  "name": "@dvanosdol88/ai-core",
  "version": "0.1.2",
  "description": "Shared AI core runtime (providers, routing, tool calling, KB interfaces).",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dvanosdol88/ai-core.git"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "clean": "node -e \"require('node:fs').rmSync('dist',{recursive:true,force:true})\"",
    "build": "npm run clean && node ./node_modules/typescript/bin/tsc -p tsconfig.build.json",
    "typecheck": "node ./node_modules/typescript/bin/tsc -p tsconfig.build.json --noEmit"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  }
}
