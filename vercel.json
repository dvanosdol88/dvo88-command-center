{
  "functions": {
    "api/index.ts": {
      "maxDuration": 30
    }
  },
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/ai-tools",
      "status": 308,
      "headers": {
        "Location": "/tools"
      }
    },
    {
      "src": "/tools",
      "dest": "/tools.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
