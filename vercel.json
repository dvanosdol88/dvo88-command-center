{
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/ai-tools",
      "status": 308,
      "headers": {
        "Location": "/tools"
      }
    },
    {
      "src": "/ai-tools\\.html",
      "status": 308,
      "headers": {
        "Location": "/tools"
      }
    },
    {
      "src": "/tools\\.html",
      "status": 308,
      "headers": {
        "Location": "/tools"
      }
    },
    {
      "src": "/dvo-ai-dev-tools\\.html",
      "status": 308,
      "headers": {
        "Location": "/tools"
      }
    },
    {
      "src": "/dvo-ai-dev-tools-v2\\.html",
      "status": 308,
      "headers": {
        "Location": "/tools"
      }
    },
    {
      "src": "/tools",
      "dest": "/tools.html"
    },
    {
      "src": "/(.*\\..*)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
