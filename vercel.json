{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/ai-tools",
      "status": 308,
      "headers": {
        "Location": "/tools"
      }
    },
    {
      "src": "/tools",
      "dest": "/tools.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
